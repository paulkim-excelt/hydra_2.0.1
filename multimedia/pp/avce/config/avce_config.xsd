<xs:element name="avce_cfg">
  <xs:complexType>
    <xs:all>
        <xs:element name="width" type="xs:positiveInteger" minOccurs="1" maxOccurs="1"/>
        <xs:element name="height" type="xs:positiveInteger" minOccurs="1" maxOccurs="1"/>
        <xs:element name="framerate" type="xs:positiveInteger" minOccurs="1" maxOccurs="1"/>
        <xs:element name="bitrate" type="xs:positiveInteger" minOccurs="1" maxOccurs="1"/>
