        <xs:element name="numcores" type="xs:positiveInteger" minOccurs="1" maxOccurs="1"/>
        <xs:element name="minqp" type="xs:positiveInteger" minOccurs="1" maxOccurs="1"/>
        <xs:element name="maxqp" type="xs:positiveInteger" minOccurs="1" maxOccurs="1"/>
        <xs:element name="slice_rows" type="xs:string" minOccurs="1" maxOccurs="1"/>
    </xs:all>
