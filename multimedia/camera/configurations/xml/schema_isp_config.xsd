        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- Color conversion -->
    <xsd:element name="color_conversion" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>
          <xsd:element name="matrix"   type="fixedp_type_s6.10_list_len_9_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="offsets"  type="fixedp_type_s18.0_list_len_3_t" minOccurs="1" maxOccurs="1"/>
        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- Low resolution resize -->
    <xsd:element name="lr_resize" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>
          <xsd:element name="rb_swap"       type="xsd:boolean"          minOccurs="0" maxOccurs="1"/>
