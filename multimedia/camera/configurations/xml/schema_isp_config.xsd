          <xsd:element name="output_height" type="fixedp_type_u32.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="shift"         type="fixedp_type_u4.0_t"   minOccurs="1" maxOccurs="1"/>
        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- High resolution output -->
    <xsd:element name="hires_output" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>
          <xsd:element name="shift"         type="fixedp_type_u4.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="big_endian"    type="xsd:boolean"         minOccurs="0" maxOccurs="1"/>
          <xsd:element name="sat_depth"     type="fixedp_type_u2.0_t"  minOccurs="0" maxOccurs="1"/>
          <xsd:element name="hflip"         type="xsd:boolean"         minOccurs="0" maxOccurs="1"/>
          <xsd:element name="vflip"         type="xsd:boolean"         minOccurs="0" maxOccurs="1"/>
          <xsd:element name="output_width"  type="fixedp_type_u1.0_t"  minOccurs="1" maxOccurs="1"/>

          <xsd:element name="data_format" minOccurs="1" maxOccurs="1">
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:enumeration value="INTERLEAVED"/>
                <xsd:enumeration value="565"/>
                <xsd:enumeration value="RGBA32_0"/>
                <xsd:enumeration value="RGBA32_255"/>
                <xsd:enumeration value="444"/>
                <xsd:enumeration value="422"/>
                <xsd:enumeration value="420"/>
                <xsd:enumeration value="YUYV"/>
                <xsd:enumeration value="YVYU"/>
                <xsd:enumeration value="UYVY"/>
                <xsd:enumeration value="VYUY"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>

        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- Low resolution output -->
    <xsd:element name="lores_output" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>
          <xsd:element name="shift"         type="fixedp_type_u4.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="big_endian"    type="xsd:boolean"         minOccurs="0" maxOccurs="1"/>
          <xsd:element name="sat_depth"     type="fixedp_type_u2.0_t"  minOccurs="0" maxOccurs="1"/>
          <xsd:element name="vflip"         type="xsd:boolean"         minOccurs="0" maxOccurs="1"/>
          <xsd:element name="output_width"  type="fixedp_type_u1.0_t"  minOccurs="1" maxOccurs="1"/>

          <xsd:element name="data_format" minOccurs="1" maxOccurs="1">
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:enumeration value="INTERLEAVED"/>
                <xsd:enumeration value="565"/>
                <xsd:enumeration value="RGBA32_0"/>
                <xsd:enumeration value="RGBA32_255"/>
                <xsd:enumeration value="444"/>
                <xsd:enumeration value="422"/>
                <xsd:enumeration value="420"/>
                <xsd:enumeration value="YUYV"/>
                <xsd:enumeration value="YVYU"/>
                <xsd:enumeration value="UYVY"/>
                <xsd:enumeration value="VYUY"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>

        </xsd:all>
      </xsd:complexType>
    </xsd:element>

  </xsd:all>
</xsd:complexType>

