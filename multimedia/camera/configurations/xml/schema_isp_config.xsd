      </xsd:complexType>
    </xsd:element>

    <!-- Color denoise -->
    <xsd:element name="color_denoise" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>

          <xsd:element name="center" minOccurs="0" maxOccurs="1">
            <xsd:complexType>
              <xsd:all>
                <xsd:element name="x" type="fixedp_type_u16.0_t" minOccurs="1" maxOccurs="1"/>
                <xsd:element name="y" type="fixedp_type_u16.0_t" minOccurs="1" maxOccurs="1"/>
              </xsd:all>
            </xsd:complexType>
          </xsd:element>

          <xsd:element name="fir_shift"             type="fixedp_type_u2.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="g_wt"                  type="fixedp_type_u4.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="bg_wt"                 type="fixedp_type_u4.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="rg_wt"                 type="fixedp_type_u4.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="iir_attenuation_lines" type="fixedp_type_u4.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="iir_attenuation_shift" type="fixedp_type_u3.0_t"  minOccurs="1" maxOccurs="1"/>
