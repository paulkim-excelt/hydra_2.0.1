    <xsd:element name="chrominance_stretch" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>
          <xsd:element name="abscissae_u"  type="fixedp_type_s16.0_list_8_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_u"  type="fixedp_type_u16.0_list_8_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
