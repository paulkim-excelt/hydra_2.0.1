          <xsd:element name="gr_neg" type="fixedp_type_s4.12_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="gb_pos" type="fixedp_type_s4.12_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="gb_neg" type="fixedp_type_s4.12_t"  minOccurs="1" maxOccurs="1"/>

          <xsd:element name="r_pos_lim"  type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="r_neg_lim"  type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="b_pos_lim"  type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="b_neg_lim"  type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="gr_pos_lim" type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="gr_neg_lim" type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="gb_pos_lim" type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="gb_neg_lim" type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>

