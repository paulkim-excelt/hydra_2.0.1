<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

<!-- Root node -->
<xsd:element name="isp_config">
  <xsd:complexType>
    <xsd:all>
      <xsd:element name="common_config"     type="isp_pipe_t" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="hv_config"         type="isp_pipe_t" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="cv_config"         type="isp_pipe_t" minOccurs="0" maxOccurs="1"/>
    </xsd:all>
  </xsd:complexType>
</xsd:element>

<!-- ISP pipeline -->
<xsd:complexType name="isp_pipe_t">
  <xsd:all>

    <!-- Input Configuration -->
    <xsd:element name="image_input" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>

        <xsd:element name="frame_width"     type="fixedp_type_u16.0_t" minOccurs="1" maxOccurs="1"/>
        <xsd:element name="frame_height"    type="fixedp_type_u16.0_t" minOccurs="1" maxOccurs="1"/>
        <xsd:element name="stride"          type="fixedp_type_u16.0_t" minOccurs="1" maxOccurs="1"/>
        <xsd:element name="transposed"      type="xsd:boolean"         minOccurs="0" maxOccurs="1"/>

        <xsd:element name="dpcm" minOccurs="0" maxOccurs="1">
          <xsd:complexType>
            <xsd:all>

              <xsd:element name="mode" minOccurs="1" maxOccurs="1">
                <xsd:simpleType>
                  <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="RAW10_8"/>
                    <xsd:enumeration value="RAW12_8"/>
                    <xsd:enumeration value="RAW14_8"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>

              <xsd:element name="blocklen"   type="fixedp_type_u9.0_t"  minOccurs="1" maxOccurs="1"/>

            </xsd:all>
          </xsd:complexType>
        </xsd:element>

        <xsd:element name="bayer_order" minOccurs="1" maxOccurs="1">
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:enumeration value="RGGB"/>
              <xsd:enumeration value="GBRG"/>
              <xsd:enumeration value="BGGR"/>
              <xsd:enumeration value="GRBG"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>

        <xsd:element name="format" minOccurs="1" maxOccurs="1">
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:enumeration value="RAW8"/>
              <xsd:enumeration value="RAW10"/>
              <xsd:enumeration value="RAW12"/>
              <xsd:enumeration value="RAW14"/>
              <xsd:enumeration value="RAW16"/>
              <xsd:enumeration value="RAW10_8"/>
              <xsd:enumeration value="RAW12_8"/>
              <xsd:enumeration value="RAW14_8"/>
              <xsd:enumeration value="RAW10L"/>
              <xsd:enumeration value="RAW12L"/>
              <xsd:enumeration value="RAW14L"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>

        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- Black level -->
    <xsd:element name="black_level" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>

          <xsd:element name="df_border_left"   type="fixedp_type_u8.0_t"  minOccurs="0" maxOccurs="1"/>
          <xsd:element name="df_border_right"  type="fixedp_type_u8.0_t"  minOccurs="0" maxOccurs="1"/>
          <xsd:element name="df_border_top"    type="fixedp_type_u8.0_t"  minOccurs="0" maxOccurs="1"/>
          <xsd:element name="df_border_bottom" type="fixedp_type_u8.0_t"  minOccurs="0" maxOccurs="1"/>
          <xsd:element name="df_mult"          type="fixedp_type_u8.8_t"  minOccurs="0" maxOccurs="1"/>
          <xsd:element name="threshold"        type="fixedp_type_u16.0_t" minOccurs="0" maxOccurs="1"/>

          <xsd:element name="long_exp" minOccurs="1" maxOccurs="1">
            <xsd:complexType>
              <xsd:all>
                <xsd:element name="lvl_r"  type="fixedp_type_u16.0_t" minOccurs="0" maxOccurs="1"/>
                <xsd:element name="lvl_b"  type="fixedp_type_u16.0_t" minOccurs="0" maxOccurs="1"/>
                <xsd:element name="lvl_gr" type="fixedp_type_u16.0_t" minOccurs="0" maxOccurs="1"/>
                <xsd:element name="lvl_gb" type="fixedp_type_u16.0_t" minOccurs="0" maxOccurs="1"/>
                <xsd:element name="abscissae_r"  type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
                <xsd:element name="ordinates_r"  type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
                <xsd:element name="abscissae_b"  type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
                <xsd:element name="ordinates_b"  type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
                <xsd:element name="abscissae_gr" type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
                <xsd:element name="ordinates_gr" type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
                <xsd:element name="abscissae_gb" type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
                <xsd:element name="ordinates_gb" type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
              </xsd:all>
            </xsd:complexType>
          </xsd:element>

          <xsd:element name="short_exp" minOccurs="0" maxOccurs="1">
            <xsd:complexType>
              <xsd:all>
                <xsd:element name="lvl_r"  type="fixedp_type_u16.0_t" minOccurs="0" maxOccurs="1"/>
                <xsd:element name="lvl_b"  type="fixedp_type_u16.0_t" minOccurs="0" maxOccurs="1"/>
                <xsd:element name="lvl_gr" type="fixedp_type_u16.0_t" minOccurs="0" maxOccurs="1"/>
                <xsd:element name="lvl_gb" type="fixedp_type_u16.0_t" minOccurs="0" maxOccurs="1"/>
                <xsd:element name="abscissae_r"  type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
                <xsd:element name="ordinates_r"  type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
                <xsd:element name="abscissae_b"  type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
                <xsd:element name="ordinates_b"  type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
                <xsd:element name="abscissae_gr" type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
                <xsd:element name="ordinates_gr" type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
                <xsd:element name="abscissae_gb" type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
                <xsd:element name="ordinates_gb" type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
              </xsd:all>
            </xsd:complexType>
          </xsd:element>

        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- Lens shading -->
    <xsd:element name="lens_shading" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:choice minOccurs="1" maxOccurs="1">

          <xsd:element name="lens_shading_use_default_params" minOccurs="1" maxOccurs="1"/>

          <xsd:sequence>
            <xsd:element name="pitch"           type="fixedp_type_u10.0_t" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="vpitch"          type="fixedp_type_u10.0_t" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="cell_sz_power_x" type="fixedp_type_u4.0_t"  minOccurs="1" maxOccurs="1"/>
            <xsd:element name="cell_sz_power_y" type="fixedp_type_u4.0_t"  minOccurs="1" maxOccurs="1"/>
            <xsd:element name="cv_r"            type="fixedp_type_u4.12_list_len_1089_t" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="cv_gr"           type="fixedp_type_u4.12_list_len_1089_t" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="cv_gb"           type="fixedp_type_u4.12_list_len_1089_t" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="cv_b"            type="fixedp_type_u4.12_list_len_1089_t" minOccurs="1" maxOccurs="1"/>
          </xsd:sequence>

        </xsd:choice>
      </xsd:complexType>
    </xsd:element>

    <!-- HDR fusion -->
    <xsd:element name="hdr_fusion" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>

          <xsd:element name="swap_exp"  type="xsd:boolean" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="thr_hi"    type="fixedp_type_u16.0_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="thr_lo"    type="fixedp_type_u16.0_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="gain_r"    type="fixedp_type_u8.8_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="gain_b"    type="fixedp_type_u8.8_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="exp_ratio" type="fixedp_type_u8.8_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="wt_sc"     type="fixedp_type_u20.0_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="threshold" type="fixedp_type_u23.0_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae" type="fixedp_type_u22.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates" type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>

        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- Bayer denoise -->
    <xsd:element name="bayer_denoise" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>

          <xsd:element name="r_str"   type="fixedp_type_u0.6_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="b_str"   type="fixedp_type_u0.6_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="gr_str"  type="fixedp_type_u0.6_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="gb_str"  type="fixedp_type_u0.6_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="r_gain"  type="fixedp_type_u2.4_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="b_gain"  type="fixedp_type_u2.4_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="gr_gain" type="fixedp_type_u2.4_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="gb_gain" type="fixedp_type_u2.4_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="mask"    type="boolean_list_len_49_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae" type="fixedp_type_u4.12_list_12_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates" type="fixedp_type_u4.12_list_12_segments_pwl_t" minOccurs="1" maxOccurs="1"/>

        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- Statistics -->
    <xsd:element name="statistics" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>

          <xsd:element name="hmask_0"           type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="hmask_1"           type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="shift"             type="fixedp_type_u8.0_t"   minOccurs="1" maxOccurs="1"/>
          <xsd:element name="pix_shift"         type="fixedp_type_u2.0_t"   minOccurs="1" maxOccurs="1"/>
          <xsd:element name="row_factor"        type="fixedp_type_u16.0_t"  minOccurs="0" maxOccurs="1"/>
          <xsd:element name="columns"           type="xsd:boolean"          minOccurs="0" maxOccurs="1"/>

          <xsd:element name="regions_group_0" minOccurs="1" maxOccurs="1">
            <xsd:complexType>
              <xsd:all>
                <xsd:element name="x_offset"    type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                <xsd:element name="y_offset"    type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                <xsd:element name="width"       type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                <xsd:element name="height"      type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                <xsd:element name="h_num"       type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                <xsd:element name="v_num"       type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
              </xsd:all>
            </xsd:complexType>
          </xsd:element>

          <xsd:element name="regions_group_1_2" minOccurs="1" maxOccurs="1">
            <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="region" minOccurs="1" maxOccurs="32">
                    <xsd:complexType>
                      <xsd:all>
                        <xsd:element      name="x_offset" type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                        <xsd:element      name="y_offset" type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                        <xsd:element      name="width"    type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                        <xsd:element      name="height"   type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      </xsd:all>
                      <xsd:attribute      name="id"       type="xsd:int"/>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
          </xsd:element>

          <xsd:element name="group_thresholds" minOccurs="1" maxOccurs="1">
            <xsd:complexType>
              <xsd:all>

                <xsd:element name="group_0" minOccurs="0" maxOccurs="1">
                  <xsd:complexType>
                    <xsd:all>
                      <xsd:element name="r_lo"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="r_hi"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="g_lo"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="g_hi"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="b_lo"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="b_hi"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="rg_lo"       type="fixedp_type_s16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="rg_hi"       type="fixedp_type_s16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="bg_lo"       type="fixedp_type_s16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="bg_hi"       type="fixedp_type_s16.0_t"  minOccurs="1" maxOccurs="1"/>
                    </xsd:all>
                  </xsd:complexType>
                </xsd:element>

                <xsd:element name="group_1" minOccurs="0" maxOccurs="1">
                  <xsd:complexType>
                    <xsd:all>
                      <xsd:element name="r_lo"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="r_hi"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="g_lo"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="g_hi"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="b_lo"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="b_hi"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="rg_lo"       type="fixedp_type_s16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="rg_hi"       type="fixedp_type_s16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="bg_lo"       type="fixedp_type_s16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="bg_hi"       type="fixedp_type_s16.0_t"  minOccurs="1" maxOccurs="1"/>
                    </xsd:all>
                  </xsd:complexType>
                </xsd:element>

                <xsd:element name="group_2" minOccurs="0" maxOccurs="1">
                  <xsd:complexType>
                    <xsd:all>
                      <xsd:element name="r_lo"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="r_hi"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="g_lo"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="g_hi"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="b_lo"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="b_hi"        type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="rg_lo"       type="fixedp_type_s16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="rg_hi"       type="fixedp_type_s16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="bg_lo"       type="fixedp_type_s16.0_t"  minOccurs="1" maxOccurs="1"/>
                      <xsd:element name="bg_hi"       type="fixedp_type_s16.0_t"  minOccurs="1" maxOccurs="1"/>
                    </xsd:all>
                  </xsd:complexType>
                </xsd:element>

              </xsd:all>
            </xsd:complexType>
          </xsd:element>

        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- White balance and gain -->
    <xsd:element name="white_balance_and_gain" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>

          <xsd:element name="gain_r"    type="fixedp_type_u8.8_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="gain_b"    type="fixedp_type_u8.8_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="gain_all"  type="fixedp_type_u8.8_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="offset_r"  type="fixedp_type_u17.0_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="offset_g"  type="fixedp_type_u17.0_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="offset_b"  type="fixedp_type_u17.0_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="threshold" type="fixedp_type_u16.0_t" minOccurs="1" maxOccurs="1"/>

        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- Defective pixel correction -->
    <xsd:element name="defective_pixel_correction" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>

          <xsd:element name="scale_lo"  type="fixedp_type_s2.4_list_len_3_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="offset_lo" type="fixedp_type_s16.0_list_len_3_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="scale_hi"  type="fixedp_type_s2.4_list_len_3_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="offset_hi" type="fixedp_type_s16.0_list_len_3_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="replace_index_lo" type="fixedp_type_u4.0_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="replace_index_hi" type="fixedp_type_u4.0_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="replace_scale_hi" type="fixedp_type_u4.4_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="replace_scale_lo" type="fixedp_type_u4.4_t" minOccurs="1" maxOccurs="1"/>

        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- Fringing -->
    <xsd:element name="fringing" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>

          <xsd:element name="r_pos"  type="fixedp_type_s4.12_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="r_neg"  type="fixedp_type_s4.12_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="b_pos"  type="fixedp_type_s4.12_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="b_neg"  type="fixedp_type_s4.12_t"  minOccurs="1" maxOccurs="1"/>
