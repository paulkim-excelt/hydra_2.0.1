          <xsd:element name="edge_strength"         type="fixedp_type_u16.0_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="edge_min"              type="fixedp_type_u16.0_t" minOccurs="1" maxOccurs="1"/>

          <xsd:element name="abscissae_fir_bg"      type="fixedp_type_u16.0_list_6_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_fir_bg"      type="fixedp_type_u6.0_list_6_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae_fir_rg"      type="fixedp_type_u16.0_list_6_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_fir_rg"      type="fixedp_type_u6.0_list_6_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae_iir_bg"      type="fixedp_type_u16.0_list_6_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_iir_bg"      type="fixedp_type_u8.0_list_6_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae_iir_rg"      type="fixedp_type_u16.0_list_6_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_iir_rg"      type="fixedp_type_u8.0_list_6_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae_dist"        type="fixedp_type_u16.0_list_6_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_dist"        type="fixedp_type_u8.0_list_6_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae_g"           type="fixedp_type_u16.0_list_6_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_g"           type="fixedp_type_u6.0_list_6_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae_sat"         type="fixedp_type_u16.0_list_6_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_sat"         type="fixedp_type_u7.0_list_6_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>

        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- Demosaic -->
    <xsd:element name="demosaic" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>

          <xsd:element name="bayer_type" minOccurs="1" maxOccurs="1">
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:enumeration value="TRUE"/>
                <xsd:enumeration value="H_COSITED"/>
                <xsd:enumeration value="V_COSITED"/>
                <xsd:enumeration value="HV_COSITED"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>

          <xsd:element name="threshold"      type="fixedp_type_u16.0_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="hv_bias"        type="fixedp_type_u9.0_t"   minOccurs="1" maxOccurs="1"/>
          <xsd:element name="g_offset_shift" type="fixedp_type_u4.0_t"   minOccurs="1" maxOccurs="1"/>

        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- Sharpen -->
    <xsd:element name="sharpen" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>

          <xsd:element name="x_center"            type="fixedp_type_u16.0_t"                      minOccurs="1" maxOccurs="1"/>
          <xsd:element name="y_center"            type="fixedp_type_u16.0_t"                      minOccurs="1" maxOccurs="1"/>
          <xsd:element name="avg_g_shift"         type="fixedp_type_u5.0_t"                       minOccurs="1" maxOccurs="1"/>
          <xsd:element name="isotropic_strength"  type="fixedp_type_u8.8_t"                       minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae_pos_gain"  type="fixedp_type_u16.0_list_8_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_pos_gain"  type="fixedp_type_u4.12_list_8_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae_v_gain"    type="fixedp_type_u16.0_list_8_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_v_gain"    type="fixedp_type_u4.12_list_8_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae_resp"      type="fixedp_type_s16.0_12_segments_pwl_t"      minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_resp"      type="fixedp_type_s16.0_12_segments_pwl_t"      minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae_resp1"     type="fixedp_type_s16.0_12_segments_pwl_t"      minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_resp1"     type="fixedp_type_s16.0_12_segments_pwl_t"      minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae_resp_gain" type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_resp_gain" type="fixedp_type_u16.0_list_16_segments_pwl_t" minOccurs="1" maxOccurs="1"/>

        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- Color correction -->
    <xsd:element name="color_correction" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>
          <xsd:element name="matrix"   type="fixedp_type_s4.12_list_len_9_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="offsets"  type="fixedp_type_s17.0_list_len_3_t" minOccurs="1" maxOccurs="1"/>
        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- Gamma correction -->
    <xsd:element name="gamma" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>

          <xsd:element name="abscissae_r" type="fixedp_type_u16.0_list_32_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_r" type="fixedp_type_u16.0_list_32_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae_g" type="fixedp_type_u16.0_list_32_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_g" type="fixedp_type_u16.0_list_32_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae_b" type="fixedp_type_u16.0_list_32_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_b" type="fixedp_type_u16.0_list_32_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>

        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- YCBCR conversion -->
    <xsd:element name="ycbcr_conversion" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>
          <xsd:element name="matrix"   type="fixedp_type_s4.10_list_len_9_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="offsets"  type="fixedp_type_s17.0_list_len_3_t" minOccurs="1" maxOccurs="1"/>
        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- False color supression -->
    <xsd:element name="false_color" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>
          <xsd:element name="strength1"       type="fixedp_type_u4.4_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="strength2"       type="fixedp_type_u4.4_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="filter_mix_cw"   type="fixedp_type_u5.3_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="filter_desat_cw" type="fixedp_type_u5.3_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="filter_mix_lo"   type="fixedp_type_u16.0_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="filter_mix_hi"   type="fixedp_type_u16.0_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="filter_desat_lo" type="fixedp_type_u16.0_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="filter_desat_hi" type="fixedp_type_u16.0_t" minOccurs="1" maxOccurs="1"/>
        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- Local tone mapping -->
    <xsd:element name="local_tone_mapping" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>
          <xsd:element name="gf_en"        type="xsd:boolean"                              minOccurs="1" maxOccurs="1"/>
          <xsd:element name="gaussian_lpf" type="fixedp_type_u18.0_list_len_81_t"          minOccurs="1" maxOccurs="1"/>
          <xsd:element name="k_hp"         type="fixedp_type_u4.4_t"                       minOccurs="1" maxOccurs="1"/>
          <xsd:element name="k_color"      type="fixedp_type_u4.4_t"                       minOccurs="1" maxOccurs="1"/>
          <xsd:element name="epsilon"      type="fixedp_type_u0.16_t"                      minOccurs="1" maxOccurs="1"/>
          <xsd:element name="div_sat"      type="fixedp_type_u32.0_t"                      minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae_b"  type="fixedp_type_u16.0_list_32_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_b"  type="fixedp_type_u16.0_list_32_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="abscissae_d"  type="fixedp_type_u16.0_list_32_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_d"  type="fixedp_type_u16.0_list_32_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
        </xsd:all>
      </xsd:complexType>
    </xsd:element>

    <!-- Tone mapping -->
    <xsd:element name="tone_mapping" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>
          <xsd:element name="abscissae_y"  type="fixedp_type_u16.0_list_32_segments_pwl_t" minOccurs="1" maxOccurs="1"/>
