    <!-- Crosstalk -->
    <xsd:element name="crosstalk" minOccurs="0" maxOccurs="1">
      <xsd:complexType>
        <xsd:all>

          <xsd:element name="abscissae_xc_lim"  type="fixedp_type_u16.0_list_8_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>
          <xsd:element name="ordinates_xc_lim"  type="fixedp_type_u16.0_list_8_segments_pwl_t"  minOccurs="1" maxOccurs="1"/>

